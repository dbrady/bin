#!/bin/bash
SCRIPT=`get-ds-script`

if [ -z "$SCRIPT" ]; then
    echo "You must set the SCRIPT variable first."
    exit -1
fi

echo git add $SCRIPT
git add $SCRIPT

echo git commit -m "Update SQL to be compatible with Snowflake in $SCRIPT"
git commit -m "Update SQL to be compatible with Snowflake in $SCRIPT"
